{
  "expo": {
    "name": "Forge",
    "slug": "forge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "forge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-nobg.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shanulabdin.forge"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "Counter",
              "label": "My Counter Widget",
              "minWidth": "180dp",
              "minHeight": "180dp",
              "targetCellWidth": 3,
              "targetCellHeight": 2,
              "description": "This is my second widget",
              "previewImage": "./assets/images/logo.png",
              "updatePeriodMillis": 1800000,
              "resizeMode": "horizontal|vertical"
            },
            {
              "name": "Score",
              "label": "Forge Score Widget",
              "description": "Overall completion percentage",
              "minWidth": "120dp",
              "minHeight": "120dp",
              "targetCellWidth": 2,
              "targetCellHeight": 2,
              "previewImage": "./assets/images/Score.png",
              "updatePeriodMillis": 1800000
            },
            {
              "name": "Streak",
              "label": "Forge Streak Widget",
              "minWidth": "120dp",
              "minHeight": "120dp",
              "targetCellWidth": 2,
              "targetCellHeight": 2,
              "description": "This is my second widget",
              "previewImage": "./assets/images/Streak.png",
              "updatePeriodMillis": 1800000
            }
          ]
        }
      ],
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3da420b0-1a0d-4b37-95d2-0b748927dbc3"
      }
    }
  }
}